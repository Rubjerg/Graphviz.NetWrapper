<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear /> <!-- This clears inherited sources -->
    <!-- public packages -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>

  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="Rubjerg.*" />
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
